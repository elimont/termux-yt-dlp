--sponsorblock-remove=sponsor,selfpromo,interaction \
--no-mtime --no-abort-on-error --playlist-reverse \
-o ~/storage/downloads/%(extractor)s/%(uploader)s/%(title)s\ -\ %(duration)s.%(ext)s \
